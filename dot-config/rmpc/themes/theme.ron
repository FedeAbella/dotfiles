    draw_borders: true,
    format_tag_separator: " | ",
    browser_column_widths: [20, 38, 42],
    background_color: "#181825",
    text_color: "#cdd6f4",
    header_background_color: "#181825",
    modal_background_color: None,
    modal_backdrop: false,
    preview_label_style: (fg: "#f9e2af"),
    preview_metadata_group_style: (fg: "#f9e2af", modifiers: "Bold"),
    tab_bar: (
        active_style: (fg: "#11111b", bg: "#cba6f7", modifiers: "Bold"),
        inactive_style: (),
    ),
    highlighted_item_style: (fg: "#fab387", modifiers: "Bold"),
    current_item_style: (fg: "#11111b", bg: "#b4befe", modifiers: "Bold"),
    borders_style: (fg: "#b4befe"),
    highlight_border_style: (fg: "#b4befe"),
    symbols: (song: "üéµ", dir: "üìÅ", playlist: "üéº", marker: "ÔÄå "),
    level_styles: (
        info: (fg: "blue", bg: "black"),
        warn: (fg: "#f9e2af", bg: "black"),
        error: (fg: "red", bg: "black"),
        debug: (fg: "light_green", bg: "black"),
        trace: (fg: "magenta", bg: "black"),
    ),
    progress_bar: (
        symbols: ["=", "=", "‚¨§", " ", " "],
        track_style: (fg: "#181825"),
        elapsed_style: (fg: "#cba6f7", bg: "#181825"),
        thumb_style: (fg: "#cba6f7", bg: "#181825"),
    ),
    scrollbar: (
        symbols: ["‚îÇ", "‚ñà", "‚ñ≤", "‚ñº"],
        track_style: (),
        ends_style: (),
        thumb_style: (fg: "#b4befe"),
    ),
    song_table_format: [
        (
            prop: (kind: Property(Artist), style: (fg: "#cdd6f4"),
                default: (kind: Text("Unknown"), style: (fg: "#cdd6f4"))
            ),
            width: "15%",
        ),
        (
            prop: (kind: Property(Title), style: (fg: "#cdd6f4"),
                default: (kind: Text("Unknown"),style: (fg: "#cdd6f4"))
            ),
            width: "50%",
        ),
        (
            prop: (kind: Property(Album), style: (fg: "#a6adc8"),
                default: (kind: Text("Unknown Album"), style: (fg: "#a6adc8"))
            ),
            width: "25%",
        ),
        (
            prop: (kind: Property(Duration), style: (fg: "#a6adc8"),
                default: (kind: Text("-"), style: (fg: "#a6adc8"))
            ),
            width: "10%",
            alignment: Right,
        ),
    ],
    layout: Split(
        direction: Vertical,
        panes: [
            (
                pane: Split(
                    direction: Horizontal,
                    panes: [
                        (
                            size: "21",
                            pane: Pane(AlbumArt),
                        ),
                        (
                            size: "100%",
                            pane: Split(
                                direction: Vertical,
                                panes: [
                                    (
                                        size: "5",
                                        pane: Pane(Header),
                                    ),
                                    (
                                        size: "1",
                                        pane: Pane(ProgressBar),
                                    ),
                                    (
                                        size: "3",
                                        pane: Pane(Tabs),
                                    ),
                                ]
                            )
                        ),
                    ]
                ),
                size: "8",
            ),
            (
                pane: Pane(TabContent),
                size: "100%",
            ),
        ],
    ),
    header: (
        rows: [
            (
                left: [
                    (kind: Property(Status(StateV2(playing_label: "ÔÅã Playing", paused_label: "ÔÅå Paused", stopped_label: "ÔÅç Stopped", playing_style: (fg: "#a6e3a1"), paused_style: (fg: "#f9e2af"), stopped_style: (fg: "#f38ba8"))))),
                ],
                center: [
                        (kind: Property(Song(Artist)), style: (fg: "#f9e2af", modifiers: "Bold"),
                            default: (kind: Text("Unknown"), style: (fg: "#f9e2af", modifiers: "Bold"))
                        ),
                ],
                right: [
                    (kind: Property(Widget(ScanStatus)), style: (fg: "#b4befe")),
                    (kind: Property(Widget(Volume)), style: (fg: "#b4befe"))
                ]
            ),
            (
                left: [
                    (kind: Property(Status(Elapsed))),
                    (kind: Text(" / ")),
                    (kind: Property(Status(Duration))),
                    (kind: Text(" (")),
                    (kind: Property(Status(Bitrate))),
                    (kind: Text(" kbps)"))
                ],
                center: [
                        (kind: Property(Song(Album)),
                            default: (kind: Text("Unknown Album"))
                        )
                ],
                right: [
                    (
                        kind: Property(Widget(States(
                            active_style: (fg: "#cdd6f4", modifiers: "Bold"),
                            separator_style: (fg: "#cdd6f4")))
                        ),
                        style: (fg: "#585b70")
                    ),
                ]
            ),
            (
                left: [],
                center: [
                    (kind: Property(Song(Title)), style: (modifiers: "Bold"),
                        default: (kind: Text("No Song"), style: (modifiers: "Bold"))
                    )
                ],
                right: []
            ),
        ],
    ),
    browser_song_format: [
        (
            kind: Group([
                (kind: Property(Track)),
                (kind: Text(" ")),
            ])
        ),
        (
            kind: Group([
                (kind: Property(Artist)),
                (kind: Text(" - ")),
                (kind: Property(Title)),
            ]),
            default: (kind: Property(Filename))
        ),
    ],
    lyrics: (
        timestamp: false
    )
)
